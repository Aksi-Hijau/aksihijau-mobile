<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.navigationmenu.home.HomeFragment">

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="20dp"
        android:text="Silakan Login untuk mendapatkan fitur menarik dari aplikasi"
        android:textAlignment="center"
        android:textSize="20sp"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/bt_login2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Login"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imageCompany"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_alignParentStart="true"
        android:layout_centerInParent="true"
        android:layout_marginTop="52dp"
        android:src="@drawable/aksihijau_logo"
        app:civ_border_color="@color/grey"
        app:civ_border_width="2dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_nama_profile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/dummy_name"
        android:textAlignment="center"
        android:layout_marginTop="20dp"
        android:textStyle="bold"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/imageCompany"/>

    <androidx.cardview.widget.CardView
        android:id="@+id/cv_admin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        app:layout_constraintTop_toBottomOf="@id/tv_nama_profile"
        app:cardCornerRadius="20dp">

        <ImageView
            android:id="@+id/iv_btn_moveadmin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/moveadmin_btn"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:layout_marginStart="60dp"/>

        <TextView
            android:id="@+id/btn_moveadmin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/to_admin"
            android:textStyle="bold"
            android:textSize="20sp"
            android:textColor="@color/green_750"
            android:layout_marginStart="120dp"
            android:layout_marginTop="20dp"/>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cv_datapribadi"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/cv_admin"
        android:layout_marginTop="40dp"
        android:padding="5dp">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/subbotton_profile1"
            android:layout_margin="20dp"/>

        <TextView
            android:id="@+id/move_profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/btn_datapribadi"
            android:layout_marginStart="60dp"
            android:layout_marginTop="20dp"
            android:textStyle="bold"/>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cv_kampanyasaya"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/cv_datapribadi"
        android:layout_marginTop="10dp"
        android:padding="5dp">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/subbotton_profile2"
            android:layout_margin="20dp"/>

        <TextView
            android:id="@+id/move_kampanyesaya"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/btn_campiagnsaya"
            android:layout_marginStart="60dp"
            android:layout_marginTop="20dp"
            android:textStyle="bold"/>

    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
        android:id="@+id/cv_logout_profile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/cv_kampanyasaya"
        android:layout_marginTop="10dp"
        android:padding="5dp">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/subbotton_profile3"
            android:layout_margin="20dp"/>

        <TextView
            android:id="@+id/move_logout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/btn_logout_profile"
            android:layout_marginStart="60dp"
            android:layout_marginTop="20dp"
            android:textStyle="bold"/>

    </androidx.cardview.widget.CardView>







</androidx.constraintlayout.widget.ConstraintLayout>